        for (var i = 0; i < children.length; i++) {
            var item = children[i].children[0].innerText + '|' + children[i].children[1].innerText + '|' +
                children[i].children[2].innerText + '|' +
                children[i].children[3].innerText + '|' +
                children[i].children[4].innerText + '|' +
