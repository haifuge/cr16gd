    <script>
        $(function () { 
            var type = getQueryString('type')

            if (type == 1) {
                $("#type1").addClass("active");
                $("#type1").siblings().removeClass("active");

                $("#tab0").addClass("active");
                $("#tab0").siblings().removeClass("active");
            }
            if (type == 2) {
                $("#type2").addClass("active");
                $("#type2").siblings().removeClass("active");
                $("#tab1").addClass("active");
                $("#tab1").siblings().removeClass("active");

            }
            if (type == 3) {
                $("#type3").addClass("active");
                $("#type3").siblings().removeClass("active");
                $("#tab2").addClass("active");
                $("#tab2").siblings().removeClass("active");

            }
            if (type == 4) {
                $("#type4").addClass("active");
                $("#type4").siblings().removeClass("active");
                $("#tab3").addClass("active");
                $("#tab3").siblings().removeClass("active");
            }
        })
            function getQueryString(name) {
                var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
                var r = window.location.search.substr(1).match(reg); //获取url中"?"符后的字符串并正则匹配
                var context = "";
                if (r != null)
                    context = r[2];
                reg = null;
                r = null;
                return context == null || context == "" || context == "undefined" ? "" : context;
            }
    </script>

<script type="text/javascript">
    //文件审批
    var cur_num = 1
    var clickpagenum = "" //所点击页码
    //当前所在页码
    var nowpage = 1;
    var page_size = 10
    var pagenum = 12
    Paginator(nowpage, page_size)
    function Paginator(page, ps) {
        $.post("/BidingFile/GetMyFileApprove", { pageSize: ps, pageIndex: page, pname: "", status: "" }, function (data) {
            if (data == 'no session') {
                location.href = "/MobileLogin";
            }
            var jsonData = JSON.parse(data);
            var obj = jsonData.List;
            console.log(obj)
            var htm = "";
            var applyStatus = "";
            var applyStyle = "";
            for (var i = 0; i < obj.length; i++) {
                var o = obj[i]
                console.log(o);
                switch (o.status) {
                    case 0:
                        applyStatus = "未发布";
                        applyStyle = "status-wcl status-wfb";
                        break;
                    case 1:
                        applyStatus = "待处理";
                        applyStyle = "status-wcl status-shz";
                        break;
                    case 2:
                        applyStatus = "已处理";
                        applyStyle = "status-wcl status-ytg";
                        break;
                    case 3:
                        applyStatus = "被驳回";
                        applyStyle = "status-wcl status-bbh";
                        break;
                    case 4:
                        applyStatus = "被暂停";
                        applyStyle = "status-wcl status-bzt";
                        break;
                }
                htm += '<tr class="white-bg" onclick = "location.href=\'/BidingFile/FileApproveDetail?pid=' + o.id + '&status=' + o.status + '\';" >' +
                    '<td class="check-color"><input class="icheck" name="id" type="checkbox" value="' + o.id + '" id="checkbox-1" /></td>' +
                    '<td>' + o.name + '</td>' +
                    '<td>' + o.content + '</td>' +
                    '<td>' + o.publisher + '</td>' +
                    '<td>' + o.publishdate + '</td>' +
                    '<td><i class="' + applyStyle + '">' + applyStatus + '</i></td>' +
                    '</tr >'
            }
            $("#tbBidingFiles").html(htm);
        });
    }
</script>

    
